NEXT_PUBLIC_API_URL=http://localhost:3001/api/v1

# Aktifkan QZ Tray untuk silent print desktop
NEXT_PUBLIC_QZ_TRAY_ENABLED=false
# Nama printer persis seperti di OS kasir (contoh: POS-58)
NEXT_PUBLIC_QZ_PRINTER=
# URL script QZ Tray (default CDN)
NEXT_PUBLIC_QZ_SCRIPT_URL=https://cdn.jsdelivr.net/npm/qz-tray@2.2.4/qz-tray.js
# Certificate public key dari QZ Tray (wajib jika QZ_TRAY_ENABLED=true)
NEXT_PUBLIC_QZ_CERTIFICATE=
# Endpoint backend untuk signing request QZ (wajib jika QZ_TRAY_ENABLED=true)
NEXT_PUBLIC_QZ_SIGNATURE_URL=

# Opsional fallback ke desktop print bridge lokal
NEXT_PUBLIC_PRINT_BRIDGE_URL=
